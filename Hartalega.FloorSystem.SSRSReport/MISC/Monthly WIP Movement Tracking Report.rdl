  <Code>Function CheckDateParametersMsg(StartDate as Date, EndDate as Date) as String
Dim msg as String
     msg = ""
     If (StartDate &gt; EndDate)  Then
 msg="Start Date should not be later than End Date"
     End If
'    If (Month(StartDate) &lt;&gt; Month(EndDate))  Then
' msg="Start Date and End Date should be same month"
'     End If
'    If (Year(StartDate) &lt;&gt; Year(EndDate)) Then
'   msg="Report date range should not cross year"
'     End If
	 	 
	 return msg
End Function

Function ShowDateParametersMsg(StartDate as Date, EndDate as Date) as Boolean
     If (StartDate &gt; EndDate)  Then
		Return "False"
     End If
'    If (Month(StartDate) &lt;&gt; Month(EndDate))  Then
'		Return "False"
'     End If
'   If (Year(StartDate) &lt;&gt; Year(EndDate)) Then
'		Return "False"
'    End If
	 
	Return "True"
End Function</Code>
