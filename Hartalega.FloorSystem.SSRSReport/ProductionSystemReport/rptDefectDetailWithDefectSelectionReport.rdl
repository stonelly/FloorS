Function ValidateDate(ByVal FromDate As Date, ByVal ToDate As Date) As String
        Dim msg As String
        Dim currentdate As Date
       currentdate =Now()
        msg = ""
        If (FromDate&gt; ToDate) Then
            msg = "From Date should not be later than To Date"
        End If   
        If (ToDate&gt; currentdate ) Then
            msg = "To Date can not be future Date"
        End If      
        If (FromDate&gt; currentdate ) Then
            msg = "From Date can not be future Date"
        End If

        Return msg 
    End Function

Function ValidatetDateBool(ByVal FromDate As Date, ByVal ToDate As Date) As Boolean
        Dim msg As Boolean
	msg = False
        Dim currentdate As Date
       currentdate =Now()       
        If (FromDate &gt; currentdate ) Then
           msg = True 
        End If       

        If (ToDate&gt; currentdate ) Then
            msg = True
        End If       

        If (FromDate &gt; ToDate) Then
            msg = True
        End If     

        Return msg 
    End Function
