Function ValidateDate(ByVal StartDate As Date, ByVal EndDate As Date) As String
        Dim msg As String
        Dim currentdate As Date
       currentdate =Now()
        msg = ""
        If (StartDate &gt; EndDate) Then
            msg = "Start Date should not be later than End Date"
        End If   
        If (EndDate&gt; currentdate ) Then
            msg = "End Date can not be future Date"
        End If      
        If (StartDate &gt; currentdate ) Then
            msg = "Start Date can not be future Date"
        End If       
 
    
        Return msg 
    End Function

Function ValidatetDateBool(ByVal StartDate As Date, ByVal EndDate As Date) As Boolean
        Dim msg As Boolean
	msg = False
        Dim currentdate As Date
       currentdate =Now()       
        If (StartDate  &gt; currentdate ) Then
           msg = True 
        End If       

        If (EndDate &gt; currentdate ) Then
            msg = True
        End If       

        If (StartDate  &gt; EndDate) Then
            msg = True
        End If     
  
        Return msg 
    End Function


Function ValidateLine(ByVal FromLine As String, ByVal ToLine As String) As String
        Dim msg As String
        Dim FromLineInt As Integer
        Dim ToLineInt As Integer
        FromLineInt =CInt(MID(FromLine, 2, 5))
        ToLineInt =CInt(MID(ToLine, 2, 5))

        msg = ""
        If (FromLineInt &gt; ToLineInt) Then
            msg = "From Line should not be larger than To Line"
        End If   
    
        Return msg 
    End Function

Function ValidatetLineBool(ByVal FromLine As String, ByVal ToLine As String) As Boolean
        Dim msg As Boolean
	msg = False
        Dim FromLineInt As Integer
        Dim ToLineInt As Integer
        FromLineInt =CInt(MID(FromLine, 2, 5))
        ToLineInt =CInt(MID(ToLine, 2, 5))


        If (FromLineInt &gt; ToLineInt) Then
            msg = True
        End If       
  
        Return msg 
    End Function











